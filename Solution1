/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */

//Brute force implementation

var twoSum = function(nums, target) {
    for(let i=0; i<nums.length ; i++){
        for(let j=i+1;j<nums.length; j++){
            let complement = target - nums[i];
            
            if(nums[j]==complement) {
                return [i, j];
            }
        }
    }
};
